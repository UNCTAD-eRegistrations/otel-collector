{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "id": 1,
      "title": "Slowest Services (p99)",
      "type": "bargauge",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "topk(10, histogram_quantile(0.99, sum by (le, service_name) (rate(traces_spanmetrics_latency_bucket[5m]))))",
          "legendFormat": "{{service_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "title": "Average Latency by Service",
      "type": "bargauge",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "topk(10, sum by (service_name) (rate(traces_spanmetrics_latency_sum[5m])) / sum by (service_name) (rate(traces_spanmetrics_latency_count[5m])))",
          "legendFormat": "{{service_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 2 }
            ]
          }
        }
      },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 0 }
    },
    {
      "id": 3,
      "title": "Latency Percentiles Over Time",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(traces_spanmetrics_latency_bucket[5m])) by (le))",
          "legendFormat": "p50 (All Services)"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(traces_spanmetrics_latency_bucket[5m])) by (le))",
          "legendFormat": "p95 (All Services)"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(traces_spanmetrics_latency_bucket[5m])) by (le))",
          "legendFormat": "p99 (All Services)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "thresholdsStyle": { "mode": "line" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 2 }
            ]
          }
        }
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 6 }
    },
    {
      "id": 4,
      "title": "Slow Traces (>2s)",
      "type": "table",
      "datasource": { "type": "tempo", "uid": "tempo" },
      "targets": [
        {
          "datasource": { "type": "tempo", "uid": "tempo" },
          "queryType": "traceqlSearch",
          "query": "{ duration > 2s }",
          "limit": 30
        }
      ],
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 14 }
    },
    {
      "id": 5,
      "title": "Database Query Latency",
      "type": "table",
      "datasource": { "type": "tempo", "uid": "tempo" },
      "targets": [
        {
          "datasource": { "type": "tempo", "uid": "tempo" },
          "queryType": "traceqlSearch",
          "query": "{ span.db.system =~ \".*\" && duration > 500ms }",
          "limit": 20
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 }
    },
    {
      "id": 6,
      "title": "HTTP Client Latency",
      "type": "table",
      "datasource": { "type": "tempo", "uid": "tempo" },
      "targets": [
        {
          "datasource": { "type": "tempo", "uid": "tempo" },
          "queryType": "traceqlSearch",
          "query": "{ kind = client && span.http.method =~ \".*\" && duration > 1s }",
          "limit": 20
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 }
    }
  ],
  "schemaVersion": 39,
  "tags": ["otel", "eregistrations", "latency", "performance", "rca"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "eRegistrations - Latency Analysis",
  "uid": "ereg-latency-analysis",
  "version": 1
}
