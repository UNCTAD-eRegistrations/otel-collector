      },
      "targets": [
        {
          "expr": "(1 - sum(rate(traces_spanmetrics_calls_total{service_name=\"camunda-boot\", status_code=\"STATUS_CODE_ERROR\"}[24h])) / sum(rate(traces_spanmetrics_calls_total{service_name=\"camunda-boot\"}[24h]))) * 100",
