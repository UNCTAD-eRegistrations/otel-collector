{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "id": 1,
      "title": "Error Rate by Service (Last Hour)",
      "type": "bargauge",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "topk(10, sum by (service_name) (rate(traces_spanmetrics_calls_total{status_code=\"STATUS_CODE_ERROR\"}[1h])) / sum by (service_name) (rate(traces_spanmetrics_calls_total[1h])) * 100)",
          "legendFormat": "{{service_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 0 }
    },
    {
      "id": 2,
      "title": "Error Count by Service",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum by (service_name) (increase(traces_spanmetrics_calls_total{status_code=\"STATUS_CODE_ERROR\"}[1h]))",
          "legendFormat": "{{service_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        }
      },
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 0 }
    },
    {
      "id": 3,
      "title": "Error Rate Over Time",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum by (service_name) (rate(traces_spanmetrics_calls_total{status_code=\"STATUS_CODE_ERROR\"}[5m])) / sum by (service_name) (rate(traces_spanmetrics_calls_total[5m])) * 100",
          "legendFormat": "{{service_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "thresholdsStyle": { "mode": "line" }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 5 }
            ]
          }
        }
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 6 }
    },
    {
      "id": 4,
      "title": "Recent Error Traces",
      "type": "table",
      "datasource": { "type": "tempo", "uid": "tempo" },
      "targets": [
        {
          "datasource": { "type": "tempo", "uid": "tempo" },
          "queryType": "traceqlSearch",
          "query": "{ status = error }",
          "limit": 50
        }
      ],
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 14 }
    },
    {
      "id": 5,
      "title": "Slow Errors (>2s)",
      "type": "table",
      "datasource": { "type": "tempo", "uid": "tempo" },
      "targets": [
        {
          "datasource": { "type": "tempo", "uid": "tempo" },
          "queryType": "traceqlSearch",
          "query": "{ status = error && duration > 2s }",
          "limit": 20
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 }
    },
    {
      "id": 6,
      "title": "HTTP 5xx Errors",
      "type": "table",
      "datasource": { "type": "tempo", "uid": "tempo" },
      "targets": [
        {
          "datasource": { "type": "tempo", "uid": "tempo" },
          "queryType": "traceqlSearch",
          "query": "{ span.http.status_code >= 500 }",
          "limit": 20
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 }
    }
  ],
  "schemaVersion": 39,
  "tags": ["otel", "eregistrations", "errors", "rca"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "eRegistrations - Error Analysis",
  "uid": "ereg-error-analysis",
  "version": 1
}
