  "id": null,
  "links": [],
  "panels": [
    {
      "id": 1,
      "title": "Error Rate by Service (Last Hour)",
      "type": "bargauge",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "topk(10, sum by (service_name) (rate(traces_spanmetrics_calls_total{status_code=\"STATUS_CODE_ERROR\"}[1h])) / sum by (service_name) (rate(traces_spanmetrics_calls_total[1h])) * 100)",
          "legendFormat": "{{service_name}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          },
          "links": [
            {
              "title": "View error traces in Explore",
              "internal": {
                "datasourceUid": "tempo",
                "datasourceName": "Tempo",
                "query": {
                  "refId": "A",
                  "queryType": "traceql",
                  "query": "{resource.service.name=\"${__field.labels.service_name}\" && status=error}",
                  "limit": 20
                }
              }
            }
          ]
        }
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Error Count by Service",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum by (service_name) (increase(traces_spanmetrics_calls_total{status_code=\"STATUS_CODE_ERROR\"}[1h]))",
          "legendFormat": "{{service_name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 10
              },
              {
                "color": "red",
                "value": 50
              }
            ]
          },
          "links": [
            {
              "title": "View error traces in Explore",
              "internal": {
                "datasourceUid": "tempo",
                "datasourceName": "Tempo",
                "query": {
                  "refId": "A",
                  "queryType": "traceql",
                  "query": "{resource.service.name=\"${__field.labels.service_name}\" && status=error}",
                  "limit": 20
                }
              }
            }
          ]
        }
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Error Rate Over Time",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum by (service_name) (rate(traces_spanmetrics_calls_total{status_code=\"STATUS_CODE_ERROR\"}[5m])) / sum by (service_name) (rate(traces_spanmetrics_calls_total[5m])) * 100",
          "legendFormat": "{{service_name}}",
          "refId": "A",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "thresholdsStyle": {
              "mode": "line"
            }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 5
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 6
      }
    },
    {
      "id": 4,
      "title": "Error Operations (by rate)",
      "type": "bargauge",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "topk(20, sum by (service_name, span_name) (rate(traces_spanmetrics_calls_total{status_code=\"STATUS_CODE_ERROR\"}[5m])))",
          "legendFormat": "{{service_name}} - {{span_name}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 14
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.5
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          },
          "links": [
            {
              "title": "View error traces for this operation",
              "internal": {
                "datasourceUid": "tempo",
                "datasourceName": "Tempo",
                "query": {
                  "refId": "A",
                  "queryType": "traceql",
                  "query": "{resource.service.name=\"${__field.labels.service_name}\" && status=error && name=~\"${__field.labels.span_name}\"}",
                  "limit": 20
                }
              }
            }
          ]
        }
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true
      }
    },
    {
      "id": 5,
      "title": "Slowest Error Operations (p99 > 2s)",
      "type": "bargauge",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "topk(10, histogram_quantile(0.99, sum by (le, service_name, span_name) (rate(traces_spanmetrics_duration_milliseconds_bucket{status_code=\"STATUS_CODE_ERROR\"}[5m]))) > 2000)",
          "legendFormat": "{{service_name}} - {{span_name}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
